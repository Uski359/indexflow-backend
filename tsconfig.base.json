{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "lib": [
      "ES2020",
      "DOM"
    ],
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "."
  },
  "exclude": [
    "node_modules",
    "dist",
    ".next",
    "build"
  ]
}
